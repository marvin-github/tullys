<p id="notice"><%= notice %></p>

<h1>Sales Plans</h1>
<div class="container-fluid">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @sales_plans.each do |sales_plan| %>
      <tr>
        <td><%= sales_plan.name %></td>
        <td><%= link_to 'Show', sales_plan, class: "btn btn-outline-dark btn-sm" %></td>
        <td><%= link_to 'Edit', edit_sales_plan_path(sales_plan), class: "btn btn-outline-dark btn-sm" %></td>
        <!--<td><%= link_to 'Destroy', sales_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sales Plan', new_sales_plan_path, :class => "btn btn-dark" %>
</div>