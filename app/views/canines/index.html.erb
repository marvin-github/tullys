<p id="notice"><%= notice %></p>

<h1>Canines</h1>

<table>
  <thead>
    <tr>
      <th>Litter</th>
      <th>Micro chip number</th>
      <th>Gender</th>
      <th>Color</th>
      <th>Price</th>
      <th>Sale price</th>
      <th>Sale status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @canines.each do |canine| %>
      <tr>
        <td><%= canine.litter.name %></td>
        <td><%= canine.micro_chip_number %></td>
        <td><%= canine.gender %></td>
        <td><%= canine.color %></td>
        <td><%= canine.price %></td>
        <td><%= canine.sale_price %></td>
        <td><%= canine.sale_status.name %></td>
        <td><%= link_to 'Show', canine %></td>
        <td><%= link_to 'Edit', edit_canine_path(canine) %></td>
        <td><%= link_to 'Destroy', canine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Canine', new_canine_path %>
