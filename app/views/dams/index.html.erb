<p id="notice"><%= notice %></p>

<h1>Dams</h1>
<div align="left">
<%= link_to 'New Dam', new_dam_path, :class => "btn btn-dark" %>
</div><br>
<div class="container-fluid">
<table class="table table-striped">
  <thead>
  <tr>
      <th>Name</th>
  </tr>
  </thead>

  <tbody>
    <% @dams.each do |dam| %>
      <tr>
        <td><%= dam.name %></td>
        <td><%= link_to 'Show', dam, class: "btn btn-outline-dark btn-sm" %>
        <td><%= link_to 'Edit', edit_dam_path(dam), class: "btn btn-outline-dark btn-sm" %>
        <!--<td><%= link_to 'Destroy', dam, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>


</div>