<p id="notice"><%= notice %></p>

<h1>Breeds</h1><br>
<div align="left">
  <%= link_to 'New Breed', new_breed_path, :class => "btn btn-dark" %>
</div>
<br>

<div class="container-fluid">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>

    </tr>
  </thead>

  <tbody>
    <% @breeds.each do |breed| %>
      <tr>
        <td><%= breed.name %></td>
        <td><%= link_to 'Show', breed, class: "btn btn-outline-dark btn-sm" %></td>
        <td><%= link_to 'Edit', edit_breed_path(breed), class: "btn btn-outline-dark btn-sm" %></td>
        <!--<td><%= link_to 'Destroy', breed, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>


</div>